{
  "name": "samuelsson-homepage",
  "version": "1.4.13",
  "description": "Personal homepage of Erik Samuelsson",
  "main": "index.js",
  "scripts": {
    "dev": "gatsby telemetry --disable && gatsby develop",
    "build": "gatsby telemetry --disable && gatsby build",
    "serve": "gatsby telemetry --disable && gatsby serve",
    "lint": "tsc --noEmit && eslint ./ --ext .js,.ts,.tsx",
    "release": "yarn standard-version && git push --follow-tags origin master",
    "prepare": "husky install"
  },
  "engines": {
    "node": "^16.0.0",
    "yarn": "^1.22.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/samuelsson/samuelsson-homepage.git"
  },
  "author": "Erik Samuelsson (https://samuelsson.dev)",
  "license": "MIT",
  "homepage": "https://samuelsson.dev",
  "eslintConfig": {
    "extends": "@samuelsson/eslint-config"
  },
  "standard-version": {
    "scripts": {
      "prerelease": "yarn lint"
    }
  },
  "dependencies": {
    "@fontsource/montserrat": "^4.4.5",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "gatsby": "^3.8.0",
    "gatsby-plugin-feed-mdx": "^1.1.0",
    "gatsby-plugin-image": "^1.8.0",
    "gatsby-plugin-mdx": "^2.8.0",
    "gatsby-plugin-react-helmet": "^4.8.0",
    "gatsby-plugin-sharp": "^3.8.0",
    "gatsby-plugin-sitemap": "^4.4.0",
    "gatsby-plugin-styled-components": "^4.8.0",
    "gatsby-remark-copy-linked-files": "^4.5.0",
    "gatsby-remark-images": "^5.5.0",
    "gatsby-remark-prismjs": "^5.5.0",
    "gatsby-source-filesystem": "^3.8.0",
    "gatsby-transformer-sharp": "^3.8.0",
    "prismjs": "^1.23.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.3.0",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@samuelsson/eslint-config": "1.6.2",
    "@types/react-helmet": "^6.1.1",
    "@types/styled-components": "^5.1.10",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "prettier": "^2.3.1",
    "standard-version": "^9.3.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  }
}
