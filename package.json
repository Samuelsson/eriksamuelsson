{
  "name": "samuelsson-homepage",
  "version": "1.6.0",
  "description": "Personal homepage of Erik Samuelsson",
  "main": "index.js",
  "scripts": {
    "dev": "gatsby telemetry --disable && gatsby develop",
    "build": "gatsby telemetry --disable && gatsby build",
    "serve": "gatsby telemetry --disable && gatsby serve",
    "lint": "tsc --noEmit && eslint ./ --ext .js,.ts,.tsx",
    "release": "yarn standard-version && git push --follow-tags origin master",
    "prepare": "husky install",
    "cm": "npx git-cz"
  },
  "engines": {
    "node": "^16.0.0",
    "yarn": "^1.22.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/samuelsson/samuelsson-homepage.git"
  },
  "author": "Erik Samuelsson (https://samuelsson.dev)",
  "license": "MIT",
  "homepage": "https://samuelsson.dev",
  "eslintConfig": {
    "extends": "@samuelsson/eslint-config"
  },
  "standard-version": {
    "scripts": {
      "prerelease": "yarn lint"
    }
  },
  "dependencies": {
    "@fontsource/montserrat": "^4.5.1",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "gatsby": "^3.13.0",
    "gatsby-plugin-feed-mdx": "^1.1.0",
    "gatsby-plugin-image": "^1.13.0",
    "gatsby-plugin-mdx": "^2.13.0",
    "gatsby-plugin-react-helmet": "^4.13.0",
    "gatsby-plugin-sharp": "^3.13.0",
    "gatsby-plugin-sitemap": "^4.9.0",
    "gatsby-plugin-styled-components": "^4.13.0",
    "gatsby-remark-copy-linked-files": "^4.10.0",
    "gatsby-remark-images": "^5.10.0",
    "gatsby-remark-prismjs": "^5.10.0",
    "gatsby-source-filesystem": "^3.13.0",
    "gatsby-styled-components-dark-mode": "^2.0.4",
    "gatsby-transformer-sharp": "^3.13.0",
    "prismjs": "^1.24.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.3.1",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@samuelsson/eslint-config": "1.6.2",
    "@types/mdx-js__react": "^1.5.4",
    "@types/react-helmet": "^6.1.2",
    "@types/styled-components": "^5.1.14",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "prettier": "^2.3.2",
    "standard-version": "^9.3.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  }
}
