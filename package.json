{
  "name": "eriksamuelsson",
  "version": "1.3.13",
  "description": "Personal homepage of Erik Samuelsson",
  "main": "index.js",
  "scripts": {
    "start": "gatsby telemetry --disable && gatsby develop",
    "develop": "gatsby telemetry --disable && gatsby develop",
    "build": "gatsby telemetry --disable && gatsby build",
    "serve": "gatsby telemetry --disable && gatsby serve",
    "lint": "tsc --noEmit && eslint ./ --ext .js,.ts,.tsx",
    "release": "yarn standard-version && git push --follow-tags origin master"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/samuelsson/eriksamuelsson.git"
  },
  "author": "@ErikSamuelsson",
  "license": "MIT",
  "homepage": "https://eriksamuelsson.com",
  "eslintConfig": {
    "extends": "@rektangel/eslint-config"
  },
  "standard-version": {
    "scripts": {
      "prerelease": "yarn lint"
    }
  },
  "dependencies": {
    "@fontsource/montserrat": "^4.2.1",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "gatsby": "^2.32.3",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-feed-mdx": "^1.1.0",
    "gatsby-plugin-mdx": "^1.10.0",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-sharp": "^2.14.1",
    "gatsby-plugin-sitemap": "^2.12.0",
    "gatsby-plugin-styled-components": "^3.10.0",
    "gatsby-remark-copy-linked-files": "^2.10.0",
    "gatsby-remark-images": "^3.11.0",
    "gatsby-remark-prismjs": "^3.13.0",
    "gatsby-source-filesystem": "^2.11.0",
    "gatsby-transformer-sharp": "^2.12.0",
    "prismjs": "^1.23.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.2.1",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rektangel/eslint-config": "1.5.1",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "prettier": "^2.2.1",
    "standard-version": "^9.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  }
}
